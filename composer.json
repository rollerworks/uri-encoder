{
    "name": "rollerworks/uri-encoder",
    "description": "A simple library, to safely encode a string for usage in a URI.",
    "keywords": ["rollerworks"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/rollerworks/uri-encoder/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/polyfill-mbstring": "^1.28"
    },
    "require-dev": {
        "ext-zlib": "*",
        "rollerscapes/standards": "^1.0",
        "phpunit/phpunit": "^10.4"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\UriEncoder\\": "src/"
        },
        "exclude-from-classmap": ["tests/"]
    },
    "autoload-dev": {
        "Rollerworks\\Component\\UriEncoder\\Tests\\": "tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
