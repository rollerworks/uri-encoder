{
    "name": "rollerworks/uri-encoder",
    "description": "A simple library, to safely encode a string for usage in a URI.",
    "keywords": ["rollerworks"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollercapes.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/Rollerworks/rollerworks-uri-encoder/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Rollerworks\\Component\\UriEncoder\\": "src/"
        },
        "exclude-from-classmap": ["spec/"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
